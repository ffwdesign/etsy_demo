<div class="center">
  <div class="row">
    <% @listings.each do |listing| %><br>
      <div class="col-md-3">
        <div class="thumbnail">
          <%= image_tag listing.image.url %>
          <div class="caption">
            <h2><%= listing.name %></h2>
            <%= number_to_currency(listing.price) %><br><br>
            <%= link_to 'Show', listing, class: "btn btn-link" %>
            <%= link_to 'Edit', edit_listing_path(listing), class: "btn btn-link" %>
            <%= link_to 'Destroy', listing, method: :delete, data: { confirm: 'Tem certeza?' }, class: "btn btn-link" %>
          </div>
        </div>
      </div>
     <% end %>
  </div>
</div>
<br>

<%= link_to 'New Listing', new_listing_path, class: "btn btn-link" %>
